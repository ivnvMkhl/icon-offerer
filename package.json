{
  "name": "icon-offerer",
  "version": "1.0.0",
  "description": "Приложение на 11ty для работы с иконками",
  "homepage": "https://ivnvMkhl.github.io/icon-offerer",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "extract-icons": "node scripts/extract-icons.js",
    "build": "npm run extract-icons && ELEVENTY_ENV=production eleventy && ELEVENTY_ENV=production npm run hash-assets",
    "hash-assets": "node scripts/hash-assets.js",
    "serve": "npm run extract-icons && ELEVENTY_ENV=production eleventy --serve",
    "dev": "npm run extract-icons && ELEVENTY_ENV=development eleventy --serve --watch",
    "start": "npm run extract-icons && ELEVENTY_ENV=production eleventy --serve"
  },
  "keywords": [
    "11ty",
    "eleventy",
    "static-site-generator",
    "icons"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "crypto": "1.0.1"
  },
  "dependencies": {
    "@ant-design/icons": "6.0.1",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "11.14.1",
    "@mui/icons-material": "7.3.2",
    "dotenv": "17.2.2"
  }
}
